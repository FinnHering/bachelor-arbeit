    \entry{kaliberaRigorousBenchmarkingReasonable2013}{inproceedings}{}{}
      \name{author}{2}{}{%
        {{hash=9aeaaaffd9b9f0bfa8dcc11c4c98813b}{%
           family={Kalibera},
