      \strng{namehash}{44cdaec3ccb4f770462a3c0f71885896}
      \strng{fullhash}{44cdaec3ccb4f770462a3c0f71885896}
      \strng{fullhashraw}{44cdaec3ccb4f770462a3c0f71885896}
      \strng{bibnamehash}{44cdaec3ccb4f770462a3c0f71885896}
